<Window x:Class="iteration3wpf.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="510" Width="736">
    <Grid>
        <Rectangle Fill="#FFF4F4F5" HorizontalAlignment="Left" Height="40" Stroke="Black" VerticalAlignment="Top" Width="728"/>
        <Menu x:Name="menuTopRight" HorizontalAlignment="Left" Height="22" Margin="653,10,0,0" VerticalAlignment="Top" Width="65" BorderBrush="#FFB4B4B4" BorderThickness="1" Background="#FFD4D4D4"/>
        <Rectangle Fill="#FFF4F4F5" HorizontalAlignment="Left" Height="440" Margin="0,39,0,0" Stroke="Black" VerticalAlignment="Top" Width="197"/>
        <TreeView x:Name="treeViewMain" HorizontalAlignment="Left" Height="367" Margin="10,103,0,0" VerticalAlignment="Top" Width="174"/>
        <ComboBox x:Name="cmbCourse" Text="Select Course" Margin="38,67,570,0" VerticalAlignment="Top" SelectionChanged="cmbCourse_SelectionChanged"/>
        <Label x:Name="lblName" Content="name" HorizontalAlignment="Left" Margin="499,-3,0,0" VerticalAlignment="Top"/>
        <Label x:Name="lblUsertype" Content="title" HorizontalAlignment="Left" Margin="499,14,0,0" VerticalAlignment="Top"/>
        <Button x:Name="btnTest" Content="Button" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="75" Click="btnTest_Click"/>
        <Rectangle Fill="#FFF4F4F5" HorizontalAlignment="Left" Height="440" Margin="197,40,0,0" Stroke="Black" VerticalAlignment="Top" Width="531"/>
        <Frame x:Name="frameMainframe" Content="Frame" HorizontalAlignment="Left" Height="440" Margin="197,39,0,0" VerticalAlignment="Top" Width="531"/>
        <Button x:Name="btnPage1" Content="Page 1" HorizontalAlignment="Left" Margin="117,10,0,0" VerticalAlignment="Top" Width="75" Click="btnPage1_Click"/>
        <Button x:Name="btnPage2" Content="Page 2" HorizontalAlignment="Left" Margin="197,10,0,0" VerticalAlignment="Top" Width="75" Click="btnPage2_Click"/>

    </Grid>
</Window>
