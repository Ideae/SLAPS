<Window x:Class="iteration3wpf.Windows.ResetPasswordWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Reset Password" Height="290" Width="264" Closed="Window_Closed">
    <Grid>
        <ListBox x:Name="listRequests" HorizontalAlignment="Left" Height="140" Margin="10,74,0,0" VerticalAlignment="Top" Width="218"/>
        <Label Content="Reset Passwords" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <Label Content="Requests" HorizontalAlignment="Left" Margin="10,48,0,0" VerticalAlignment="Top"/>
        <Button x:Name="btnResetPassword" Content="Reset Password" HorizontalAlignment="Left" Margin="10,219,0,0" VerticalAlignment="Top" Width="97" Click="btnResetPassword_Click"/>
        <Button x:Name="btnCancel" Content="Close" HorizontalAlignment="Left" Margin="153,219,0,0" VerticalAlignment="Top" Width="75" Click="btnCancel_Click"/>

    </Grid>
</Window>
